@model personal_blog.Models.Entity.TblExperience
@{
    ViewBag.Title = "getExperience";
    Layout = "~/Views/Shared/_AdminLayouth.cshtml";
}
<h2>Deneyim Guncelleme Sayfası</h2>
<br />
<form method="post">
    @Html.Label("Deneyim ID")
    @Html.TextBoxFor(x => x.Id, new { @class = "form-control" })<br />
    @Html.Label("Baslık")
    @Html.TextBoxFor(x => x.title, new { @class = "form-control" })<br />
    @Html.Label("Alt Başlık")
    @Html.TextBoxFor(x => x.subtitle, new { @class = "form-control" })<br />
    @Html.Label("Açıklama")
    @Html.TextAreaFor(x => x.explain, new { @class = "form-control" })<br />
    @Html.Label("Date")
    @Html.TextBoxFor(x => x.date, new { @class = "form-control" })<br />


    <button class="btn btn-info">Guncelle</button>

</form>

